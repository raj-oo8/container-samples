@model List<WeatherForecastV2>

@{
    ViewData["Title"] = "Weather";
}
<h1>@ViewData["Title"]</h1>

<div>API result</div>
<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Temperature (C)</th>
            <th>Humidity</th>
            <th>Summary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var forecast in Model)
        {
            <tr>
                <td>@forecast.Date</td>
                <td>@forecast.TemperatureC</td>
                <td>@forecast.Humidity</td>
                <td>@forecast.Summary</td>
            </tr>
        }
    </tbody>
</table>
